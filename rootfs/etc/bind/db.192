;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	wrt.lan. admin.wrt.lan. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	wrt.lan.
1.1	IN	PTR	wrt.lan.
; Fill your reverse loopbacks here.
; 1.201	IN	PTR	server.lan.
