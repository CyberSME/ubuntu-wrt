;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	armada.local. admin.armada.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	armada.local.
1.1	IN	PTR	armada.local.
; Fill your reverse loopbacks here.
; 1.10	IN	PTR	server.local.
